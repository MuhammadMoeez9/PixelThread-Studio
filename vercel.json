{
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "pixelthreadstudio.com"
                }
            ],
            "destination": "https://www.pixelthreadstudio.com/:path*",
            "permanent": true
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}